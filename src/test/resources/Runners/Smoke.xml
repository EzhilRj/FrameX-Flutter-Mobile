<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Suite">
    <listeners>
        <listener class-name="Listeners.FrameX_Listeners"></listener>
    </listeners>
    <test name="Smoke Tests">
        <groups>
            <run>
                <include name="smoke" />
            </run>
        </groups>
        <classes>
            <class name="Tests.LoginTest" />
            <class name="Tests.AttendanceTest" />
            <class name="Tests.CallPlanTest" />
        </classes>
    </test>
</suite>
