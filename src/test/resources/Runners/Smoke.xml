<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="SMOKE SUITE" verbose="3">
    <listeners>
        <listener class-name="Listeners.FrameX_Listeners"></listener>
    </listeners>
    <test name="LoginTest">
        <classes>
            <class name="Tests.LoginTest">
                <methods>
                    <include name="TC001_VerifyLogin" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="AttendanceTest">
        <classes>
            <class name="Tests.AttendanceTest">
                <methods>
                    <include name="TC002_VerifyAttendance" />
                </methods>
            </class>
        </classes>
    </test>

    <test name="CallplanTest">
        <classes>
            <class name="Tests.CallPlanTest">
                <methods>
                    <include name="TC001_VerifyUploadcall" />
                </methods>
            </class>
        </classes>
    </test>

</suite>